@model AccountDataModel
@{
    var doc =(AccountDataModel)ViewData["DocModel"];
    
}
<p1> Документ успішно додано до архіву </p1>
<br />
<br />
<p1> Зображення документа </p1>
<br />
<br />
<img src="..//image/de.jpg" alt="DExist" width="100" height="150">
<br />
<br />
<table style="width:40%">
    <tr>
        <td>Назва документу</td>
        <td> @ViewData["Name"]</td>
    </tr>
    <tr>
        <td>Для кого документ</td>
        <td>@ViewData["Organiz"]</td>
    </tr>
    <tr>
        <td>Тип документа за змістом</td>
        <td>@ViewData["Doctype"]</td>
    </tr>
    <tr>
        <td>Емайл-адреса бухгалтера</td>
        <td>@ViewData["Email"]</td>
    </tr>
    <tr>
        <td>Дата фомування</td>
        <td>@ViewData["Day"] . @ViewData["Month"] . @ViewData["Year"]</td>
    </tr>
</table><br /><br /><br /><br />

<button type="button"  onclick="location.href='@Url.Action("Index","Home")'"> Повернутися на головну сторінку </button>